{
  "name": "osint",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/osint/test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/bull": "^0.6.2",
    "@nestjs/common": "^8.0.0",
    "@nestjs/core": "^8.0.0",
    "@nestjs/microservices": "^9.2.1",
    "@nestjs/platform-express": "^8.0.0",
    "@ntegral/nestjs-sentry": "^3.0.7",
    "@sentry/node": "^7.28.1",
    "@sentry/tracing": "^7.28.1",
    "aws-lambda": "^1.0.7",
    "bull": "^4.10.2",
    "cache-manager": "^4.1.0",
    "crawlee": "^3.1.4",
    "ioredis": "^5.2.4",
    "mime-types": "^2.1.35",
    "pdf-lib": "^1.17.1",
    "pdf-parse": "^1.1.1",
    "puppeteer-core": "^19.11.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0",
    "tesseract.js": "^3.0.3",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "@nestjs/cli": "^8.0.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.4.7",
    "@sentry/hub": "^7.28.1",
    "@types/bull": "^3.15.9",
    "@types/cache-manager": "^4.0.2",
    "@types/express": "^4.17.13",
    "@types/jest": "27.5.0",
    "@types/mime-types": "^2.1.1",
    "@types/node": "^16.18.11",
    "@types/pdf-parse": "^1.1.1",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^8.3.4",
    "@types/word-extractor": "^1.0.1",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "jest": "28.0.3",
    "serverless-offline": "^12.0.4",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "28.0.1",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.0.0",
    "tslib": "^2.4.1",
    "typescript": "^4.9.5",
    "webpack": "^5.75.0"
  },
  "peerDependencies": {
    "@sentry/hub": "^7.6.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "^@app/browser-module(|/.*)$": "<rootDir>/libs/browser-module/src/$1"
    }
  }
}