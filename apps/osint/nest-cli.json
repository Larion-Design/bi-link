{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/osint/src",
  "monorepo": true,
  "root": "apps/osint",
  "generateOptions": {
    "spec": false
  },
  "compilerOptions": {
    "webpack": true
  },
  "projects": {
    "scraper-termene": {
      "type": "application",
      "root": "apps/scraper-termene",
      "entryFile": "main",
      "sourceRoot": "apps/scraper-termene/src",
      "compilerOptions": {
        "tsConfigPath": "apps/scraper-termene/tsconfig.app.json"
      }
    },
    "browser-module": {
      "type": "library",
      "root": "libs/browser-module",
      "entryFile": "index",
      "sourceRoot": "libs/browser-module/src",
      "compilerOptions": {
        "tsConfigPath": "libs/browser-module/tsconfig.lib.json"
      }
    }
  }
}