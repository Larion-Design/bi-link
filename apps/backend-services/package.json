{
  "name": "backend-services",
  "version": "0.0.9",
  "description": "BI Link backend services",
  "author": "alextudor@duck.com",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "ingress": "nest start ingress --watch",
    "build:ingress": "nest build ingress",
    "build": "pnpm build:api && pnpm build:indexer && pnpm build:graph && pnpm build:files-parser && pnpm build:user-actions-recorder pnpm build:termene && pnpm build:cli",
    "api": "nest start api --watch",
    "build:api": "nest build api",
    "indexer": "nest start indexer --watch",
    "build:indexer": "nest build indexer",
    "files-manager": "nest start files-manager --watch",
    "build:files-manager": "nest build files-manager",
    "graph": "nest start graph --watch",
    "build:graph": "nest build graph",
    "user-actions-recorder": "nest start user-actions-recorder --watch",
    "build:user-actions-recorder": "nest build user-actions-recorder",
    "cli": "nest start cli --watch",
    "build:cli": "nest build cli",
    "termene": "nest start osint-termene --watch",
    "build:termene": "nest build osint-termene",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "lint": "eslint --ext=.ts --quiet ./",
    "test": "jest --passWithNoTests"
  },
  "dependencies": {
    "@elastic/elasticsearch": "^8.9.0",
    "@nestjs/apollo": "^12.0.7",
    "@nestjs/bullmq": "^10.0.1",
    "@nestjs/cache-manager": "^2.1.0",
    "@nestjs/common": "^10.2.1",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^10.2.1",
    "@nestjs/elasticsearch": "^10.0.1",
    "@nestjs/graphql": "^12.0.8",
    "@nestjs/jwt": "^10.1.0",
    "@nestjs/microservices": "^10.2.1",
    "@nestjs/mongoose": "^10.0.1",
    "@nestjs/passport": "^10.0.1",
    "@nestjs/platform-express": "^10.2.1",
    "@ntegral/nestjs-sentry": "^4.0.0",
    "@sentry/node": "^7.64.0",
    "@sentry/tracing": "^7.64.0",
    "apollo-server-core": "^3.12.0",
    "apollo-server-express": "^3.12.0",
    "bullmq": "^4.8.0",
    "cache-manager": "^5.2.3",
    "cache-manager-ioredis-yet": "^1.2.2",
    "compression": "^1.7.4",
    "date-fns": "^2.30.0",
    "defs": "workspace:*",
    "firebase-admin": "^11.10.1",
    "graphql": "^16.8.0",
    "helmet": "^5.1.1",
    "ioredis": "^5.3.2",
    "jwks-rsa": "^2.1.5",
    "mime-types": "^2.1.35",
    "minio": "^7.1.1",
    "mongoose": "^6.12.0",
    "multer": "1.4.5-lts.1",
    "nats": "^2.16.0",
    "neo4j-driver": "^5.11.0",
    "nest-commander": "^3.11.1",
    "nest-neo4j": "^0.2.0",
    "nestjs-minio-client": "^1.2.0",
    "newrelic": "latest",
    "passport": "^0.5.3",
    "passport-firebase-jwt": "^1.2.1",
    "passport-jwt": "^4.0.1",
    "pdf-lib": "^1.17.1",
    "pdf-parse": "^1.1.1",
    "puppeteer-core": "^19.11.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "tesseract.js": "^3.0.3",
    "uuid": "^8.3.2",
    "word-extractor": "^1.0.4",
    "xlsx": "^0.18.5",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "@firebase/app-types": "^0.8.1",
    "@nestjs/cli": "^10.1.16",
    "@nestjs/schematics": "^10.0.2",
    "@nestjs/testing": "^10.2.1",
    "@sentry/hub": "^7.64.0",
    "@types/compression": "^1.7.2",
    "@types/elasticsearch": "^5.0.40",
    "@types/express": "^4.17.17",
    "@types/inquirer": "^9.0.3",
    "@types/jest": "27.5.0",
    "@types/mime-types": "^2.1.1",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.18.46",
    "@types/passport-jwt": "^3.0.9",
    "@types/pdf-parse": "^1.1.1",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^8.3.4",
    "@types/word-extractor": "^1.0.2",
    "express": "^4.18.2",
    "jest": "28.0.3",
    "nest-commander-testing": "^3.3.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "tools": "workspace:*",
    "ts-jest": "28.0.1",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "4.0.0",
    "tslib": "^2.6.2",
    "typescript": "^4.9.5",
    "webpack": "^5.88.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "^@app/rpc(|/.*)$": "<rootDir>/libs/rpc/src/$1",
      "^@app/service-health(|/.*)$": "<rootDir>/libs/service-health/src/$1",
      "^@app/definitions(|/.*)$": "<rootDir>/libs/definitions/src/$1",
      "^@app/scheduler-module(|/.*)$": "<rootDir>/libs/scheduler-module/src/$1",
      "^@app/loader-module(|/.*)$": "<rootDir>/libs/loader-module/src/$1",
      "^@app/diff-module(|/.*)$": "<rootDir>/libs/diff-module/src/$1",
      "^@app/cache(|/.*)$": "<rootDir>/libs/cache/src/$1"
    }
  },
  "peerDependencies": {
    "@firebase/app-types": "^0.8.0",
    "@sentry/hub": "^7.6.0",
    "@types/inquirer": "^9.0.3"
  }
}