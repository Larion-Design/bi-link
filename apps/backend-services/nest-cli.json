{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/api/src",
  "monorepo": true,
  "compilerOptions": {
    "webpack": true,
    "tsConfigPath": "tsconfig.json"
  },
  "projects": {
    "api": {
      "type": "application",
      "root": "apps/api",
      "entryFile": "main",
      "sourceRoot": "apps/api/src",
      "compilerOptions": {
        "tsConfigPath": "apps/api/tsconfig.app.json"
      }
    },
    "indexer": {
      "type": "application",
      "root": "apps/indexer",
      "entryFile": "main",
      "sourceRoot": "apps/indexer/src",
      "compilerOptions": {
        "tsConfigPath": "apps/indexer/tsconfig.app.json"
      }
    },
    "entities": {
      "type": "library",
      "root": "libs/entities",
      "entryFile": "index",
      "sourceRoot": "libs/entities/src",
      "compilerOptions": {
        "tsConfigPath": "libs/entities/tsconfig.lib.json"
      }
    },
    "pub": {
      "type": "library",
      "root": "libs/pub",
      "entryFile": "index",
      "sourceRoot": "libs/pub/src",
      "compilerOptions": {
        "tsConfigPath": "libs/pub/tsconfig.lib.json"
      }
    },
    "service-health": {
      "type": "library",
      "root": "libs/service-health",
      "entryFile": "index",
      "sourceRoot": "libs/service-health/src",
      "compilerOptions": {
        "tsConfigPath": "libs/service-health/tsconfig.lib.json"
      }
    },
    "files-parser": {
      "type": "application",
      "root": "apps/files-parser",
      "entryFile": "main",
      "sourceRoot": "apps/files-parser/src",
      "compilerOptions": {
        "tsConfigPath": "apps/files-parser/tsconfig.app.json"
      }
    },
    "definitions": {
      "type": "library",
      "root": "libs/definitions",
      "entryFile": "index",
      "sourceRoot": "libs/definitions/src",
      "compilerOptions": {
        "tsConfigPath": "libs/definitions/tsconfig.lib.json"
      }
    },
    "user-actions-recorder": {
      "type": "application",
      "root": "apps/user-actions-recorder",
      "entryFile": "main",
      "sourceRoot": "apps/user-actions-recorder/src",
      "compilerOptions": {
        "tsConfigPath": "apps/user-actions-recorder/tsconfig.app.json"
      }
    },
    "files": {
      "type": "library",
      "root": "libs/files",
      "entryFile": "index",
      "sourceRoot": "libs/files/src",
      "compilerOptions": {
        "tsConfigPath": "libs/files/tsconfig.lib.json"
      }
    },
    "graph-module": {
      "type": "library",
      "root": "libs/graph-module",
      "entryFile": "index",
      "sourceRoot": "libs/graph-module/src",
      "compilerOptions": {
        "tsConfigPath": "libs/graph-module/tsconfig.lib.json"
      }
    },
    "graph": {
      "type": "application",
      "root": "apps/graph",
      "entryFile": "main",
      "sourceRoot": "apps/graph/src",
      "compilerOptions": {
        "tsConfigPath": "apps/graph/tsconfig.app.json"
      }
    },
    "cli": {
      "type": "application",
      "root": "apps/cli",
      "entryFile": "main",
      "sourceRoot": "apps/cli/src",
      "compilerOptions": {
        "tsConfigPath": "apps/cli/tsconfig.app.json"
      }
    },
    "scheduler-module": {
      "type": "library",
      "root": "libs/scheduler-module",
      "entryFile": "index",
      "sourceRoot": "libs/scheduler-module/src",
      "compilerOptions": {
        "tsConfigPath": "libs/scheduler-module/tsconfig.lib.json"
      }
    }
  }
}