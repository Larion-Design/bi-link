{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/api/src",
  "monorepo": true,
  "compilerOptions": {
    "webpack": true
  },
  "projects": {
    "api": {
      "type": "application",
      "root": "apps/api",
      "entryFile": "main",
      "sourceRoot": "apps/api/src",
      "compilerOptions": {
        "tsConfigPath": "apps/api/tsconfig.app.json"
      }
    },
    "indexer": {
      "type": "application",
      "root": "apps/indexer",
      "entryFile": "main",
      "sourceRoot": "apps/indexer/src",
      "compilerOptions": {
        "tsConfigPath": "apps/indexer/tsconfig.app.json"
      }
    },
    "rpc": {
      "type": "library",
      "root": "libs/rpc",
      "entryFile": "index",
      "sourceRoot": "libs/rpc/src",
      "compilerOptions": {
        "tsConfigPath": "libs/rpc/tsconfig.lib.json"
      }
    },
    "service-health": {
      "type": "library",
      "root": "libs/service-health",
      "entryFile": "index",
      "sourceRoot": "libs/service-health/src",
      "compilerOptions": {
        "tsConfigPath": "libs/service-health/tsconfig.lib.json"
      }
    },
    "definitions": {
      "type": "library",
      "root": "libs/definitions",
      "entryFile": "index",
      "sourceRoot": "libs/definitions/src",
      "compilerOptions": {
        "tsConfigPath": "libs/definitions/tsconfig.lib.json"
      }
    },
    "user-actions-recorder": {
      "type": "application",
      "root": "apps/user-actions-recorder",
      "entryFile": "main",
      "sourceRoot": "apps/user-actions-recorder/src",
      "compilerOptions": {
        "tsConfigPath": "apps/user-actions-recorder/tsconfig.app.json"
      }
    },
    "files": {
      "type": "library",
      "root": "libs/files",
      "entryFile": "index",
      "sourceRoot": "libs/files/src",
      "compilerOptions": {
        "tsConfigPath": "libs/files/tsconfig.lib.json"
      }
    },
    "graph": {
      "type": "application",
      "root": "apps/graph",
      "entryFile": "main",
      "sourceRoot": "apps/graph/src",
      "compilerOptions": {
        "tsConfigPath": "apps/graph/tsconfig.app.json"
      }
    },
    "cli": {
      "type": "application",
      "root": "apps/cli",
      "entryFile": "main",
      "sourceRoot": "apps/cli/src",
      "compilerOptions": {
        "tsConfigPath": "apps/cli/tsconfig.app.json"
      }
    },
    "scheduler-module": {
      "type": "library",
      "root": "libs/scheduler-module",
      "entryFile": "index",
      "sourceRoot": "libs/scheduler-module/src",
      "compilerOptions": {
        "tsConfigPath": "libs/scheduler-module/tsconfig.lib.json"
      }
    },
    "ingress": {
      "type": "application",
      "root": "apps/ingress",
      "entryFile": "main",
      "sourceRoot": "apps/ingress/src",
      "compilerOptions": {
        "tsConfigPath": "apps/ingress/tsconfig.app.json"
      }
    },
    "files-manager": {
      "type": "application",
      "root": "apps/files-manager",
      "entryFile": "main",
      "sourceRoot": "apps/files-manager/src",
      "compilerOptions": {
        "tsConfigPath": "apps/files-manager/tsconfig.app.json"
      }
    }
  }
}