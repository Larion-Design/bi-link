{
  "name": "backend",
  "projectType": "application",
  "implicitDependencies": ["docker"],
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "dependsOn": ["^build"],
      "options": {
        "cwd": "{projectRoot}",
        "command": "ncc build src/main.ts -o dist -m -s -q --v8-cache -e pdf-parse"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    }
  }
}
