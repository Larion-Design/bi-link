{
  "root": true,
  "env": {
    "node": true,
    "jest": true,
    "es6": true,
    "shared-node-browser": true,
    "browser": true
  },
  "parserOptions": {
    "project": ["packages/tools/tsconfig.eslint.json"],
    "extraFileExtensions": [".json"],
    "sourceType": "module",
    "allowImportExportEverywhere": true,
    "ecmaVersion": "latest"
  },
  "rules": {
    "@typescript-eslint/no-empty-interface": "off"
  }
}
