{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "start": {
      "cache": false,
      "dependsOn": ["backend", "frontend"]
    },
    "docker:start": {
      "cache": false,
      "dependsOn": ["^docker:start"]
    },
    "docker:stop": {
      "cache": false,
      "dependsOn": ["^docker:stop"]
    },
    "backend": {
      "cache": false,
      "dependsOn": ["start"]
    },
    "frontend": {
      "cache": false,
      "dependsOn": ["^dev"]
    },
    "native-app": {
      "cache": false,
      "dependsOn": ["^native:dev"]
    },
    "lint": {
      "cache": true,
      "dependsOn": ["^lint"],
      "outputs": []
    },
    "test": {
      "cache": true,
      "dependsOn": ["^test"],
      "outputs": []
    },
    "build": {
      "cache": true,
      "dependsOn": ["^build"],
      "outputs": []
    }
  }
}