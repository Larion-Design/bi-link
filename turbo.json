{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "dev": {
      "cache": false,
      "dependsOn": ["backend", "frontend"]
    },
    "docker:start": {
      "cache": false,
      "dependsOn": ["^docker:start"]
    },
    "docker:stop": {
      "cache": false,
      "dependsOn": ["^docker:stop"]
    },
    "backend": {
      "cache": false,
      "dependsOn": ["ingress", "api", "graph", "indexer", "files-manager", "user-actions-recorder", "termene"]
    },
    "frontend": {
      "cache": false,
      "dependsOn": ["^dev"]
    },
    "native-app": {
      "cache": false,
      "dependsOn": ["^native:dev"]
    },
    "ingress": {
      "cache": false,
      "dependsOn": ["^ingress"]
    },
    "api": {
      "cache": false,
      "dependsOn": ["^api"]
    },
    "indexer": {
      "cache": false,
      "dependsOn": ["^indexer"]
    },
    "files-manager": {
      "cache": false,
      "dependsOn": ["^files-manager"]
    },
    "graph": {
      "cache": false,
      "dependsOn": ["^graph"]
    },
    "user-actions-recorder": {
      "cache": false,
      "dependsOn": ["^user-actions-recorder"]
    },
    "cli": {
      "cache": false,
      "dependsOn": ["^cli"]
    },
    "termene": {
      "cache": false,
      "dependsOn": ["^termene"]
      
    },
    "lint": {
      "cache": true,
      "dependsOn": ["^lint"],
      "outputs": []
    },
    "test": {
      "cache": true,
      "dependsOn": ["^test"],
      "outputs": []
    },
    "build": {
      "cache": true,
      "dependsOn": ["^build"],
      "outputs": []
    }
  }
}